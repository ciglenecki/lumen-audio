				"parameters": [
					{
						"required": false,
						"schema": {
							"title": "Model Ckpt Path",
							"type": "string",
							"format": "path",
							"default": "models/model.ckpt"
						},
						"name": "model_ckpt_path",
						"in": "query"
					}
				],
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"title": "Dataset Paths",
								"type": "array",
								"items": {
									"type": "string",
									"format": "path"
								},
								"default": [
									"data/my_dataset"
								]
							}
						}
					}
				},
				"responses": {
					"200": {
						"description": "Successful Response",
						"content": {
							"application/json": {
								"schema": {
									"title": "Response Predict Directory Stream Model Predict Directory Stream Post",
									"type": "object",
